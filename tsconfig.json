{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "esModuleInterop": true,
      "moduleResolution": "Node",
      "lib": ["DOM", "ESNext"],
      "strict": true,
      "sourceMap": true,
      "jsx": "react-jsx",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true
    },
    "ts-node": {
      "compilerOptions": {
        "target": "ES2015",
        "module": "CommonJS"
      }
    }
  }